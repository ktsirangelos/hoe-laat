<div
  class=""
>
  <div class="">
    <div class="">
      <h2 class="">
        hoe laat
      </h2>
    </div>

    <%= form_for(resource, as: resource_name, url: session_path(resource_name), html: { class: "space-y-6 w-full" }) do |f| %>
      <div class="">
        <div>
          <%= f.label :email, class: "" %>
          <%= f.email_field :email,
                        autofocus: true,
                        autocomplete: "email",
                        class:
                          "",
                        placeholder: "you@example.com" %>
        </div>

        <div>
          <%= f.label :password, class: "" %>
          <%= f.password_field :password,
                           autocomplete: "current-password",
                           class:
                             "",
                           placeholder: "Enter your password" %>
        </div>

        <% if devise_mapping.rememberable? %>
          <div class="">
            <%= f.check_box :remember_me,
                        class:
                          "" %>
            <%= f.label :remember_me, class: "" %>
          </div>
        <% end %>
      </div>

      <div>
        <%= f.submit "Sign in",
                 class:
                   "" %>
      </div>
    <% end %>

    <div class="">
      <%= link_to "Forgot your password?",
      new_password_path(resource_name),
      class: "" %>
      <%= link_to "Sign up",
      new_registration_path(resource_name),
      class: "" %>
    </div>
  </div>
</div>
